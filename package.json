{
  "name": "mucky",
  "version": "1.0.0",
  "description": "A MU* client in the browser",
  "main": "backend/index.js",
  "dependencies": {
    "autosize": "^4.0.0",
    "classnames": "^2.2.5",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.19.4",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "react-linkify": "^0.2.1",
    "react-scroll": "^1.7.4",
    "socket.io": "^1.7.2",
    "split": "^1.0.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-prettier": "^2.0.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-scss": "^2.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2"
  },
  "scripts": {
    "pretty:file": "prettier --write --",
    "css:lint:file": "stylelint --",
    "css:fix:file": "npm-run-all \"frontend:css:lint -- {@}\" \"frontend:css:lint -- {@}\" --",
    "js:lint:file": "eslint --",
    "js:fix:file": "eslint --fix --",
    "frontend:css:lint": "npm run --silent css:lint:file -- frontend/**/*.scss",
    "frontend:css:pretty": "npm run --silent pretty:file -- frontend/**/*.scss",
    "frontend:css:fix": "npm run --silent css:fix:file --frontend/**/*.scss",
    "frontend:css:clean": "rimraf frontend/index.css",
    "frontend:css:compile": "node-sass-chokidar frontend -o frontend",
    "frontend:css:build": "npm-run-all frontend:css:fix frontend:css:compile",
    "frontend:css:watch:only": "node-sass-chokidar frontend -o frontend --watch --recursive",
    "frontend:css:watch": "npm-run-all frontend:css:build frontend:css:watch:only",
    "frontend:js:lint": "npm run --silent js:lint:file -- frontend/**/*.js",
    "frontend:js:pretty": "npm run --silent pretty:file -- frontend/**/*.js",
    "frontend:js:fix": "npm run --silent js:fix:file -- frontend/**/*.js",
    "frontend:js:clean": "rimraf frontend/build",
    "frontend:js:build:production": "cross-env NODE_ENV=production webpack --config frontend/webpack.config.js -p",
    "frontend:clean": "npm-run-all frontend:css:clean frontend:js:clean",
    "frontend:build": "npm-run-all frontend:css:build frontend:css:build",
    "backend:js:lint": "npm run --silent js:lint:file -- backend/**/*.js",
    "backend:js:pretty": "npm run --silent pretty:file -- backend/**/*.js",
    "backend:js:fix": "npm run --silent js:fix:file -- backend/**/*.js",
    "backend:js:build": "npm-run-all backend:js:fix",
    "backend:build": "npm-run-all backend:js:build",
    "backend:start": "node backend/index.js",
    "backend:start:prod": "cross-env NODE_ENV=production node backend/index.js",
    "start:dev": "npm-run-all frontend:clean -p frontend:css:watch backend:start",
    "start": "npm-run-all frontend:build backend:build backend:start:prod",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run js:fix:file",
      "git add"
    ],
    "*.scss": [
      "npm run css:fix:file",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/postfurry/mucky.git"
  },
  "author": "Morgan Nichols",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/postfurry/mucky/issues"
  },
  "homepage": "https://github.com/postfurry/mucky#readme",
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
}
