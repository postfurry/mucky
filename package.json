{
  "name": "mucky",
  "version": "1.0.0",
  "description": "A MU* client in the browser",
  "main": "init.js",
  "dependencies": {
    "autosize": "^4.0.0",
    "depromisify": "^1.0.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "socket.io": "^1.7.2",
    "xkcd-password": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar public/css -o public/css",
    "watch-css-only": "node-sass-chokidar public/css -o public/css --watch --recursive",
    "watch-css": "npm-run-all build-css watch-css-only",
    "start-js-dev": "node init.js",
    "clean-js": "rimraf public/js/bundle.js public/js/bundle.js.map",
    "start-dev": "npm-run-all clean-js -p watch-css start-js-dev",
    "build-js": "cross-env NODE_ENV=production webpack -p",
    "start-js-prod": "cross-env NODE_ENV=production node init.js",
    "start": "npm-run-all build-js start-js-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/postfurry/mucky.git"
  },
  "author": "Morgan Nichols",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/postfurry/mucky/issues"
  },
  "homepage": "https://github.com/postfurry/mucky#readme"
}
