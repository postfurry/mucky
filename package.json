{
  "name": "mucky",
  "version": "1.0.0",
  "description": "A MU* client in the browser",
  "main": "init.js",
  "dependencies": {
    "autosize": "^4.0.0",
    "classnames": "^2.2.5",
    "ejs": "^2.5.5",
    "eslint-config-standard-preact": "^1.1.6",
    "express": "^4.14.0",
    "js-cookie": "^2.2.0",
    "preact": "^8.2.7",
    "socket.io": "^1.7.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar frontend -o frontend",
    "watch-css-only": "node-sass-chokidar frontend -o frontend --watch --recursive",
    "watch-css": "npm-run-all build-css watch-css-only",
    "clean-frontend-js": "rimraf public/js/bundle.js public/js/bundle.js.map",
    "build-frontend-js": "cross-env NODE_ENV=production webpack -p",
    "build-frontend": "npm-run-all build-css build-frontend-js",
    "start-backend-dev": "node init.js",
    "start-dev": "npm-run-all clean-frontend-js -p watch-css start-backend-dev",
    "start-backend-prod": "cross-env NODE_ENV=production node init.js",
    "start": "npm-run-all build-frontend start-backend-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/postfurry/mucky.git"
  },
  "author": "Morgan Nichols",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/postfurry/mucky/issues"
  },
  "homepage": "https://github.com/postfurry/mucky#readme",
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
}
